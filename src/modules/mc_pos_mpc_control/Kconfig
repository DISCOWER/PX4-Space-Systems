menuconfig MODULES_MC_POS_MPC_CONTROL
	bool "mc_pos_mpc_control"
	default n
	---help---
		Enable support for mc_pos_mpc_control

menuconfig USER_MC_POS_MPC_CONTROL
	bool "mc_pos_mpc_control running as userspace module"
	default n
	depends on BOARD_PROTECTED && MODULES_MC_POS_MPC_CONTROL
	---help---
		Put mc_pos_mpc_control in userspace memory
