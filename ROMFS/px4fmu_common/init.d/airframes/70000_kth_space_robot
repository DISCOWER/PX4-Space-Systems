param set-default PWM_MAIN_DIS3 0
param set-default PWM_MAIN_DIS4 0
param set-default PWM_MAIN_DIS5 0
param set-default PWM_MAIN_DIS6 0
param set-default PWM_MAIN_DIS7 0
param set-default PWM_MAIN_DIS8 0

param set-default PWM_MAIN_MIN1 0
param set-default PWM_MAIN_MIN2 0
param set-default PWM_MAIN_MIN3 0
param set-default PWM_MAIN_MIN4 0
param set-default PWM_MAIN_MIN5 0
param set-default PWM_MAIN_MIN6 0
param set-default PWM_MAIN_MIN7 0
param set-default PWM_MAIN_MIN8 0

param set-default PWM_MAIN_MAX1 2500
param set-default PWM_MAIN_MAX2 2500
param set-default PWM_MAIN_MAX3 2500
param set-default PWM_MAIN_MAX4 2500
